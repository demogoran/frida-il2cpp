{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "lib": ["es2020"],
    "allowJs": false,
    "noEmit": false,
    "strict": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*", "src/*"]
    }
  },
  "exclude": [
    "./node_modules/*",
    "../frida-il2cpp-bridge-old/*",
    "../frida-il2cpp-bridge-master/*",
    "../frida-Il2Cpp-bridge-master/*"
  ]
}
